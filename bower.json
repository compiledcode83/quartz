{
  "name": "quartz",
  "version": "0.1.9",
  "description": "Quartz is a javascript utility for producing responsive pinterest-style layouts with balanced column heights.",
  "homepage": "https://github.com/r-park/quartz",
  "repository": "https://github.com/r-park/quartz.git",
  "author": "Richard Park",
  "license": "MIT",
  "dependencies": {
    "matchMedia": "*"
  },
